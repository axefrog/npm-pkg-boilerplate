{
  "name": "npm-package-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate NPM Package",
  "scripts": {
    "test": "mocha --opts ./mocha.opts",
    "test-dev": "mocha --opts ./mocha.opts --watch --bail",
    "test-dev-mem": "node --max_old_space_size=8192 ./node_modules/mocha/bin/_mocha --opts ./mocha.opts --watch --bail",
    "serve-docs": "browser-sync start --server --files ./docs --ss ./docs"
  },
  "main": "lib/es2015/index.js",
  "module": "lib/es2015/index.js",
  "jsnext:main": "lib/es2015/index.js",
  "typings": "lib/es2015/index.d.ts",
  "files": [
    "lib"
  ],
  "author": "Nathan Ridley <axefrog@gmail.com>",
  "license": "MIT",
  "bugs": "https://github.com/axefrog/npm-pkg-boilerplate/issues",
  "repository": "git@github.com:axefrog/npm-pkg-boilerplate.git",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.38",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-mocha": "^3.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-transform": "^1.1.0",
    "gulp-tslint": "^7.0.1",
    "gulp-typedoc": "^2.0.2",
    "gulp-typescript": "^3.1.4",
    "merge2": "^1.0.3",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "source-map-support": "^0.4.11",
    "tslint": "^4.4.2",
    "typedoc": "^0.5.5",
    "typescript": "^2.1.5"
  }
}
